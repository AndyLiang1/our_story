{
    "singleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": true,
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
